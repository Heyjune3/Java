package abstract_base;

public class Car {
	
	private abstract void drive();
	
	private abstract void stop();
	
	private void startCar() {
		System.out.println("시동을 켭니다.");
	}
	
	private void turnOff() {
		System.out.println("시동을 끕니다.");
	}
	
	public final void run() {
		startCar();
		
		turnOff();
	}

}
